<configure
  xmlns="http://namespaces.zope.org/zope"
  xmlns:browser="http://namespaces.zope.org/browser"
  xmlns:i18n="http://namespaces.zope.org/i18n"
  i18n_domain="collective.wtforms">

  <browser:page
    name="test-form"
    for="*"
    class="..testing.TestFormView"
    permission="zope2.View"
  />
  <browser:page
    name="test-fieldset"
    for="*"
    class="..testing.TestFieldsetView"
    permission="zope2.View"
  />
  <browser:page
    name="test-controlpanel-form"
    for="*"
    class="..testing.TestControlPanelView"
    permission="zope2.View"
  />

</configure>